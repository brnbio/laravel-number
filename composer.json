{
    "name": "brnbio/laravel-number",
    "description": "Number helper for Laravel framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Heider",
            "email": "heider@brnb.io"
        }
    ],
    "minimum-stability": "dev",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Brnbio\\LaravelNumber\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brnbio\\LaravelNumber\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brnbio\\LaravelNumber\\NumberServiceProvider"
            ]
        }
    }
}
